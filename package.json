{
  "name": "reactshoppe-api",
  "version": "0.1.0",
  "bin": {
    "reactshoppe-api": "bin/reactshoppe-api.js"
  },
  "scripts": {
    "build": "tsc && npm run build:webpack",
    "build:webpack": "webpack --config packages/functions/webpack.config.js --display-error-details",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap": "lerna link && lerna exec -- npm i",
    "deploy": "npm run bootstrap && npm run build && cdk deploy"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "10.17.5",
    "aws-cdk": "2.50.0",
    "jest": "^25.5.0",
    "lerna": "^3.22.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.1.0",
    "typescript": "~4.8.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "@types/aws-lambda": "^8.10.53",
    "aws-lambda": "^1.0.6",
    "source-map-support": "^0.5.16",
    "functions": "file:packages/functions",
    "reactshoppe-api": "file:packages/reactshoppe-api",
    "reactshoppe-database": "file:packages/reactshoppe-database"
  }
}
